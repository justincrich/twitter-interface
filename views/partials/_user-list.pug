.grid-33
  div(class="app--section")
    ul.app--user--list
      each friend in friendData
        -var pic = friend.image
        li
          .circle--fluid
            .circle--fluid--cell.circle--fluid--primary
              a.app--tweet--author
                .app--avatar(style="background-image: url("+pic+")")
                  img(src=pic)
                h4 #{friend.name}
                p #{friend.username}
            .circle--fluid--cell
              a.button.button-text Unfollow
